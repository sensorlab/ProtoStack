    <!-- http://localhost/owl/crime.owl#c_channel -->

    <owl:NamedIndividual rdf:about="http://localhost/owl/crime.owl#c_channel">
        <rdf:type rdf:resource="&cpan;Module"/>
        <rdfs:comment>The channel module is the basic module of any stack and ensures the proper opening and closing of a communication channel. The value of the communication channel must be provided by the user as input.</rdfs:comment>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_abc_input"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_channel_close"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_channel_open"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_rime_output"/>
