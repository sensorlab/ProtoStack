    <!-- http://localhost/owl/crime.owl#c_mesh -->

    <owl:NamedIndividual rdf:about="http://localhost/owl/crime.owl#c_mesh">
        <rdf:type rdf:resource="&cpan;Module"/>
        <hasParameter>hop_no</hasParameter>
        <rdfs:comment> The mesh module sends packets using multi-hop routing to a specified receiver somewhere in the network. The mesh module uses 3 channel; one for the multi-hop forwarding and two for the route disovery.</rdfs:comment>
        <hasParameter>originator</hasParameter>
        <hasParameter>seq_no</hasParameter>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#attrlist"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_mesh_close"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_mesh_forward"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_mesh_open"/>
