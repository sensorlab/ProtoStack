    <!-- http://localhost/owl/crime.owl#c_abc -->

    <owl:NamedIndividual rdf:about="http://localhost/owl/crime.owl#c_abc">
        <rdf:type rdf:resource="&cpan;Module"/>
        <rdfs:comment>The abc module is designed to send annonymous broadcast messages on the selected channel. It uses three parameters chammelno, attrlist and num_tx. Channel_no configures the channel, attrlist determines the header of the packet while num_tx stores the number of attempted transmissions. All parameters are set in other modules.</rdfs:comment>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#attrlist"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_abc_close"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_abc_open"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_abc_recv"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_abc_send"/>
        <defines rdf:resource="http://localhost/owl/crime.owl#c_abc_sent"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#channel_no"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#num_tx"/>
        <hasScope rdf:resource="http://localhost/owl/crime.owl#singlehop"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#time_trigger_flg"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#trigger_interval"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#trigger_no"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#trigger_th"/>
    </owl:NamedIndividual>
    


