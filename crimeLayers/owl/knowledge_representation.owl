        <hasParameter rdf:resource="http://localhost/owl/crime.owl#channel_no"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#num_tx"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#sender"/>
        <hasScope rdf:resource="http://localhost/owl/crime.owl#singlehop"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#time_trigger_flg"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#trigger_interval"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#trigger_no"/>
        <hasParameter rdf:resource="http://localhost/owl/crime.owl#trigger_th"/>
